<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Registrar Enrolado <i class="fa fa-users"></i></h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Enrolado
                </div>

                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <form role="form" method="POST" action="/enrolados/agregarEnrolado">

                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>RUT Cliente</label>
                                            <input type="number" class="form-control" name="RutPer_e" maxlength="11" required
                                                placeholder="262632008" value="11111107">
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Municipio</label>
                                            <select class="form-control" name="IdMuni_e" maxlength="20" required name="IdMed">
                                                {{#each data.municipios}}
                                                <option value="{{IdMuni}}">{{NombreMunic}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <button class="btn btn-success">Guardar</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /.container-fluid -->