<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Registrar Precios <i class="fa fa-usd" ></i></h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Precios
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">

                            <form role="form" method="POST" action="/listaPrecios/agregadoPrecio">
                                



                                <div class="row">
                                    <div class="col-lg-3">

                                        <div class="form-group">
                                            <label>Id medicamento</label>
                                            <select class="form-control" id="NomMed" maxlength="20" required name="NomMed">
                                                <option hidden >Id medicamento</option>
                                                {{#each data.medicamentos}}
                                                <option value="{{IdMed}}">{{IdMed}} - {{NomMed}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Descuento</label>
                                            <select class="form-control" id="AplicSN" maxlength="20" required name="AplicSN">
                                                <option hidden >Aplica/No aplica</option>
                                                {{#each data.descuentos}}
                                                <option value="{{AplicSN}}">{{AplicSN}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-3">

                                        <div class="form-group">
                                            <label>Precio</label>
                                            <input class="form-control" id="precio" type="number" min="100" required name="precio">
                                        </div>

                                    </div>





                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Formato MED</label>
                                            <select class="form-control" id="TipoFormato" maxlength="20" required name="TipoFormato">
                                                <option hidden >Caja/Frasco/Tableta</option>
                                                {{#each data.formatos}}
                                                <option value="{{TipoFormato}}">{{TipoFormato}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Presentaci√≥n MED</label>
                                            <select class="form-control" id="TipoPresentacion" maxlength="20" required name="TipoPresentacion">
                                                <option hidden >gr/mg/ml</option>
                                                {{#each data.presentaciones}}
                                                <option value="{{TipoPresentacion}}">{{TipoPresentacion}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Tipo MED</label>
                                            <select class="form-control" id="TipoMedic" maxlength="20" required name="TipoMedic">
                                                <option hidden >Con Receta/Sin Receta/Receta retenida</option>
                                                {{#each data.tipos}}
                                                <option value="{{TipoMedic}}">{{TipoMedic}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                        

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <button class="btn btn-success">Guardar</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /.container-fluid -->