            <h1 class="page-header">Registrar Vencimiento <i class="fa fa-calendar"></i></h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Vencimiento
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <form role="form" method="POST" action="/vencimiento/agregarVencimiento">

                                <div class="row">
                                    <!--
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Proveedor</label>
                                            <select class="form-control" id="prov" maxlength="20" required name="IdProv_v">
                                                {{#each proveedores}}
                                                <option value="{{IdProv}}">{{NomLab}}</option>
                                                {{/each}}
                                            </select>
                                            
                                        </div>
                                    </div>-->
                                    


                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Unidades</label>
                                            <input type="number" min="1" class="form-control" id="units" maxlength="20" required name="Unidades">
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Lote</label>
                                            <input type= "number" min="1" class="form-control" id="lote" required name="IdLote_v">
                                               
                                            
                                        </div>
                                    </div>

                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Fecha Vencimiento</label>
                                            <input type= "date" class="form-control" id="fechavenc" required name="FechaVencimiento">
                                               
                                            
                                        </div>
                                    </div>

                        
                                        

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <button class="btn btn-success">Guardar</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
