<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Registrar Quimico Farmaceuta <i class="fa fa-user fa-fw"></i></h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Quimico Farmaceuta
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <form role="form" method="POST" action="/quimicos/agregarQuimicoFarm">

                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>RUT</label>
                                            <input type="number" class="form-control" id="quiRut" required placeholder="262632008" min="1" max="999999999" name="RutQuimic">
                                        </div>
                                    </div>

                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <label>Nombre</label>
                                            <input class="form-control" id="quiNom" maxlength="20" required name="NomQuimic">
                                        </div>
                                    </div>

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <button class="btn btn-success">Guardar</button>
                                        </div>
                                    </div>
                                </row>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /.container-fluid -->